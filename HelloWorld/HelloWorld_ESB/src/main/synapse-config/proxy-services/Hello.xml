<?xml version="1.0" encoding="UTF-8"?>
<proxy name="Hello" startOnLoad="true" trace="disable"
  transports="http https" xmlns="http://ws.apache.org/ns/synapse">
  <target>
    <inSequence>
      <payloadFactory media-type="xml">
        <format>
          <response xmlns="">Hello!</response>
        </format>
        <args/>
      </payloadFactory>
      <respond/>
    </inSequence>
    <outSequence/>
    <faultSequence/>
  </target>
</proxy>
